ALTER TABLE hiveconfig RENAME TO sqlitestudio_temp_table;

CREATE TABLE hiveconfig (
    HOMEDIR                              VARCHAR,
    YARDID                               INTEGER,
    HIVENAME                             VARCHAR,
    HIVEID,
    BEEKEEPERID,
    ENABLE_HIVE_TEMP_CHK,
    HIVEDEVICE,
    ENABLE_HIVE_WEIGHT_CHK,
    WEATHER_LEVEL,
    WEATHER_DETAIL,
    [KEY],
    CITY,
    STATE,
    WXSTATION,
    DISPLAY_TYPE,
    PUBLIC_HTML_DIR,
    SHARE_HIVETOOL,
    COUNTRY,
    LATITUDE,
    LONGITUDE,
    SCALETYPE,
    TEMPTYPE,
    NUM_HIVE_BASE_SOLID_BOTTOM_BOARD,
    NUM_HIVE_BASE_SCREENED_BOTTOM_BOARD,
    NUM_HIVE_FEEDER,
    NUM_HIVE_TOP_INNER_COVER,
    NUM_HIVE_TOP_TELE_COVER,
    NUM_HIVE_TOP_MIGRATORY_COVER,
    NUM_HIVE_BODY_MEDIUM_FOUNDATION,
    NUM_HIVE_BODY_MEDIUM_FOUNDATION_LESS,
    NUM_HIVE_BODY_DEEP_FOUNDATION,
    NUM_HIVE_BODY_DEEP_FOUNDATION_LESS,
    NUM_HIVE_BODY_SHAL_FOUNDATION,
    NUM_HIVE_BODY_SHAL_FOUNDATION_LESS,
    VERSION,
    LOG,
    DATECONFIG,
    ID,
    TIMEZONE,
    HT_USERNAME,
    HT_PASSWORD,
    HT_URL,
    ENABLE_HIVE_CAMERA,
    GDD_BASE_TEMP,
    GDD_START_DATE,
    www_chart_theme,
    ENABLE_LUX,
    LUX_SOURCE,
    NASA_HONEYBEE_NET_ID,
    POWER,
    INTERNET,
    STATUS,
    COMPUTER,
    START_DATE,
    HIVE_TEMP_GPIO,
    HIVE_TEMP_MEASURE,
    HIVE_WEIGHT_SLOPE,
    HIVE_WEIGHT_INTERCEPT,
    SITE_ORIENT,
    SITE_TYPE,
    WXTEMPTYPE,
    WX_TEMP_GPIO,
    WX_TEMPER_DEVICE,
    check_for_upgrades,
    upgrade_available
);

